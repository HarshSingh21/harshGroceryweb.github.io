<script>
var baby_prod =  scid = pcid = 0,
ApiDomainName = "https://www.firstcry.com/";
function babygearWar(pid,page) {
    baby_prod = pid;
    $(".baby-gear-review-popup-bg").hide();
    if(page == "combo") {
        ComboProductList = ComboDetailJson.CProList;
        prod_thumb = [];
        for (var i = 0; i < ComboProductList.length; i++) {
            if (baby_prod == ComboProductList[i].PID.toString()) {
                pcid = ComboProductList[i].CatID
                scid = ComboProductList[i].Scid;
                pgrpid = ComboProductList[i].PgID;
                prod_thumb[0] = baby_prod + "a.webp";
                prod_name = ComboProductList[i].PN;
                callCommonPopup(scid,prod_thumb[0],prod_name,pid,pgrpid,pcid);
            }
        }
    } else if(page == "pdp") {
        pcid = ProductDetailJSON.PInfo.PCID;
        scid = ProductDetailJSON.PInfo.SCID;
        pgrpid = ProductDetailJSON.PInfo.PgID;
        prod_thumb = (CurrentProductDetailJSON[pid]['Img']).split(";");
        prod_name = CurrentProductDetailJSON[pid].pn;
        callCommonPopup(scid,prod_thumb[0],prod_name,pid,pgrpid,pcid);
    } else if(page == "reviews") {
        pcid = ProductDetailJSON.PInfo.PCID;
        scid = ProductDetailJSON.PInfo.SCID;
        pgrpid = ProductDetailJSON.PInfo.PgID;
        prod_thumb = (ProductDetailJSON['PInfo']['Img']).split(";");
        prod_name = ProductDetailJSON['PInfo']['pnm'];
        callCommonPopup(scid,prod_thumb[0],prod_name,pid,pgrpid,pcid);
    } else {
        let user_id = getCookie("_$FC_UserInfo$_");
        if (user_id == "" || user_id == undefined || user_id == null) {
            user_id = 0;
        }
        var url = ApiDomainName + "svcs/CommonService.svc/getProduct/pid="  + pid + "/uid=" + user_id;        
        $.ajax({
            url: url,
            type: "get",
            datatype: "json",
            contenttype: "application/json; charset=utf-8",
            success: function (result) {
                if (result != "" && result != null) {
                    pcid = result.PInfo.PCID;
                    scid = result.PInfo.SCID;
                    pgrpid = (result.PInfo.PgID != '')? result.PInfo.PgID : '';
                    prod_thumb = (result['PInfo']['Img']).split(";");
                    prod_name = result.PInfo.pnm;
                    callCommonPopup(scid,prod_thumb[0],prod_name,pid,pgrpid,pcid);
                    $(".baby-gear-review-popup-bg").show();
                }
            }
        });
