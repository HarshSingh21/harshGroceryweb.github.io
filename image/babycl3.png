function callCommonPopup(scid,prod_thumb,prod_name,pid,pgrpid,pcid) {
    var UserId = getCookie("_$FC_UserInfo$_");    
    $(".rev-thankyou-popup .rev-head").html("Thanks for your review of " + prod_name);
    $(".rev-thankyou-popup .email-text").html(getCookie("_$FC_LoginInfo$_"));    
    if (prod_name.length > 50) {
        prod_name = prod_name.substring(0, 50) + "...";
    }    
    if (UserId == "" || UserId == undefined || UserId == null) {
        UserId = 0;
    }
    if (pgrpid == "" || pgrpid == undefined || pgrpid == null) {
        pgrpid = 0;
    }
    var addRating = "https://www.firstcry.com/api/pdp/getadditionalrating";
    var post_data = '{"productid":"' + pid + '", "ftk": "' + getCookie("FC_AUTH") + '"}';
    $.ajax({
        url: addRating,
        type: "POST",
        data: post_data,
        contentType: "application/json",
        header: {'Content-Type': 'application/json', 'Content-Length': post_data.length },
        dataType: "json",
        success: function (result1) {
            if (result1 != "" && result1 != null) {
                let ass_rate = (result1.AdditionalRatings["assembly and handling"] != undefined)? result1.AdditionalRatings["assembly and handling"]: 0;
                let com_rate = (result1.AdditionalRatings["comfortness"] != undefined)? result1.AdditionalRatings["comfortness"]: 0;
                let dur_rate = (result1.AdditionalRatings["durability"] != undefined)? result1.AdditionalRatings["durability"]: 0;
                let qty_rate = (result1.AdditionalRatings["quality"] != undefined)? result1.AdditionalRatings["quality"]: 0;
                let saf_rate = (result1.AdditionalRatings["safety"] != undefined)? result1.AdditionalRatings["safety"]: 0;
                let mon_rate = (result1.AdditionalRatings["value for money"] != undefined)? result1.AdditionalRatings["value for money"]: 0;

                $('.star-assembly').attr("data-rate", ass_rate);
                starMouseOver(ass_rate, 'assm-');                
                $('.star-quality').attr("data-rate", qty_rate);
                starMouseOver(qty_rate, 'qty-');                
                $('.star-safety').attr("data-rate", saf_rate);
                starMouseOver(saf_rate, 'sef-');                
                $('.star-com').attr("data-rate", com_rate);
                starMouseOver(com_rate, 'com-');                
                $('.star-dur').attr("data-rate", dur_rate);
                starMouseOver(dur_rate, 'dur-');                
                $('.star-mon').attr("data-rate", mon_rate);
                starMouseOver(mon_rate, 'mon-');

                var PURL = ApiDomainName + "svcs/CommonService.svc/getPdSizesQueReviewdata/pid=" + pid + "/uid=" + UserId + "/grpid=" + pgrpid + "?isclub=" + fcclubflag;
                $.ajax({
                    url: PURL,
                    type: "get",
                    datatype: "json",
                    contenttype: "application/json; charset=utf-8",
                    success: function (result) {
                        if (result != "" && result != null) {
                            var PURL = ApiDomainName + "svcs/RatingReview.svc/GetUserReview_v1";
                            let Data = '{"UserID": "' + UserId + '", "ProductID": "' + pid + '", "ftk": "' + getCookie("FC_AUTH") + '"}';
                            $.ajax({
                                url: PURL,
                                type: "POST",
                                data: Data,
                                contentType: "application/json",
                                header: {'Content-Type': 'application/json', 'Content-Length': Data.length },
                                dataType: "json",
                                success: function (res) {
                                    $(".baby-gear-title,.baby-thank-title").html((result.isapproved == 1)? "EDIT A REVIEW": 'WRITE A REVIEW');
                                    $(".line-1").html(prod_name);                            
                                    $(".col-1 img").attr("src", "//cdn.fcglcdn.com/brainbees/images/products/438x531/" + (prod_thumb).replace(".jpg", ".webp"));
                                    if (res != "" && res != null) {
                                        if (res.UserReview != "" && res.UserReview != null) {
                                            $.each(res.UserReview, function (k, v) {
                                                result.Urating = v.rating;
                                                $("#babyname").val(v.UserName);
                                                $("#babytitle").val(v.ReviewTitle);
                                                $("#babymessage").val(v.ReviewData.replace("-", " ").replace("_", " "));
                                            });
                                        }
                                    }

                                    if(($("#babyname").val()).trim() == "") {
                                        var PURL = ApiDomainName + "svcs/MyAccountService.svc/getUserDetail_app";
                                        let Data = '{"fcauth": "' + getCookie("FC_AUTH") + '"}';
                                        $.ajax({
                                            url: PURL,
                                            type: "POST",
                                            data: Data,
                                            contentType: "application/json",
                                            dataType: "json",
                                            success: function (res2) {
                                                if (res2 != "" && res2 != null) {
                                                    if(res2['UserMaster'] != undefined && res2['UserMaster']['firstname'] != undefined) {
                                                        $("#babyname").val(res2['UserMaster']['firstname'] + " " + res2['UserMaster']['lastname']);
                                                    }
                                                }
                                            }
                                        });
                                    }

                                    if(result.Urating != null && result.Urating > 0) {
                                        if($(".baby-star").attr("data-rate") <=0) {
                                            $(".baby-star").attr("data-rate", result.Urating);
                                        }                                        
                                        if(result.Urating > 0) {
                                            $(".star-1").addClass("y-star");
                                        }
                                        if(result.Urating > 1) {
                                            $(".star-2").addClass("y-star");
                                        }
                                        if(result.Urating > 2) {
                                            $(".star-3").addClass("y-star");
                                        }
                                        if(result.Urating > 3) {
                                            $(".star-4").addClass("y-star");
                                        }
                                        if(result.Urating > 4) {
                                            $(".star-5").addClass("y-star");
                                        }
                                    }                        
                                    $(".popup-bg").fadeIn(200);
                                    (pcid == 7)? $(".baby-gear-review-popup .babygear-star-sec, .baby-gear-review-popup .review-text").show() : $(".baby-gear-review-popup .babygear-star-sec, .baby-gear-review-popup .review-text").hide();
                                    $(".baby-gear-review-popup").show();
                                },
                            });
                        }
                    },
                });
            }
        }
    });
}

function starMouseOver(starval, classval) {
    if(starval != null && starval > 0) {
        if(starval > 0) {
            $("." + classval + "1").addClass("y-star");
        }
        if(starval > 1) {
            $("." + classval + "2").addClass("y-star");
        }
        if(starval > 2) {
            $("." + classval + "3").addClass("y-star");
        }
        if(starval > 3) {
            $("." + classval + "4").addClass("y-star");
        }
        if(starval > 4) {
            $("." + classval + "5").addClass("y-star");
        }
    } 
}

